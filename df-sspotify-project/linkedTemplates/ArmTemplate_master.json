{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-sspotify-project"
		},
		"AzureSqlDatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase'"
		},
		"azuredatalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'azuredatalake'"
		},
		"AzureSqlDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "spotifyprojectservers.database.windows.net"
		},
		"AzureSqlDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "spotify-azure-db"
		},
		"AzureSqlDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminsindh"
		},
		"azuredatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sspotifyazureproject.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-sspotify-project_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase_password": {
						"value": "[parameters('AzureSqlDatabase_password')]"
					},
					"azuredatalake_accountKey": {
						"value": "[parameters('azuredatalake_accountKey')]"
					},
					"AzureSqlDatabase_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase_properties_typeProperties_userName')]"
					},
					"azuredatalake_properties_typeProperties_url": {
						"value": "[parameters('azuredatalake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}